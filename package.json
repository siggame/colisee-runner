{
  "name": "colisee-runner",
  "version": "0.0.0",
  "description": "Run queued match.",
  "main": "dist/src/app.js",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "http-errors": "^1.6.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cors": "^2.8.1",
